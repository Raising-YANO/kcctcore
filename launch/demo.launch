<launch>
    <arg name="sim" default="true" />
    <arg name="map_2d" default="true" />
    <arg name="show_gui" default="true"/>
    <arg name="way" default="wpdataA.csv"/>
    <arg name="world_name" default="$(find kcctsim)/worlds/clearpath_playpen.world"/>

    <include file="$(find kcctcore)/launch/navigationA.launch">
        <arg name="sim" default="$(arg sim)" />
        <arg name="map_2d" default="$(arg map_2d)"/>
        <arg name="show_gui" default="$(arg show_gui)"/>
        <arg name="way" default="$(arg way)"/>
        <arg name="world_name" default="$(arg world_name)"/>
    </include>
</launch>